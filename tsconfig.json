{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "ESNext",
        "target": "es2022",
        "lib": [
            "es2022",
            "dom"
        ],
        "types": [
            "node",
            "@wdio/globals/types",
            "expect-webdriverio",
            "@wdio/mocha-framework"
        ],
        "skipLibCheck": true,
        "noEmit": true,
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "sourceMap": true,
        "esModuleInterop": true
    },
    "include": [
        "test",
        "base.wdio.conf.ts",
        "semilocal.wdio.conf.ts",
        "selenoid.wdio.conf.ts",
        "remote.seq.wdio.conf.ts",
        "export.wdio.conf.ts",
    ]
}